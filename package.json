{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "sync": "ncp ./README.md ./content/AUTO_GEN_README.md",
    "build": "pnpm sync && nuxt build",
    "dev": "pnpm sync && nuxt dev",
    "generate": "pnpm sync && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "gitpagesbuild": "pnpm sync && nuxt build --preset github_pages",
    "prepare": "husky"
  },
  "dependencies": {
    "@nuxt/content": "~2.12.1",
    "mermaid": "^10.9.0",
    "ncp": "^2.0.0",
    "nuxt": "~3.11.1",
    "primeicons": "~6.0.1",
    "primevue": "~3.50.0",
    "vue": "~3.4.21",
    "vue-router": "~4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.0.3",
    "@nuxtjs/color-mode": "^3.4.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nuxt-primevue": "^0.3.1",
    "prettier": "^3.2.5",
    "sass": "^1.75.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "pnpm": ">=8.6.10"
